<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D1F0AA"
    tools:context=".Profile">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="214dp"
        android:layout_height="42dp"
        android:layout_x="128dp"
        android:layout_y="417dp"
        android:background="#B1B0B0" />

    <TextView
        android:id="@+id/emailpro"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_x="145dp"
        android:layout_y="424dp"
        android:text="test@gmail.com"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="159dp"
        android:layout_y="90dp"
        android:text="Profile"
        android:textSize="24sp" />

    <Button
        android:id="@+id/logoutBT"
        android:layout_width="329dp"
        android:layout_height="54dp"
        android:layout_x="33dp"
        android:layout_y="546dp"
